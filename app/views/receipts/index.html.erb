<p id="notice"><%= notice %></p>

<h1>Listing Receipts</h1>

<table>
  <thead>
    <tr>
      <th>Skey</th>
      <th>Tran</th>
      <th>Domain</th>
      <th>Currency</th>
      <th>Paydate</th>
      <th>Orderid</th>
      <th>Appcode</th>
      <th>Error code</th>
      <th>Error desc</th>
      <th>Channel</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @receipts.each do |receipt| %>
      <tr>
        <td><%= receipt.skey %></td>
        <td><%= receipt.tran_id %></td>
        <td><%= receipt.domain %></td>
        <td><%= receipt.currency %></td>
        <td><%= receipt.paydate %></td>
        <td><%= receipt.orderid %></td>
        <td><%= receipt.appcode %></td>
        <td><%= receipt.error_code %></td>
        <td><%= receipt.error_desc %></td>
        <td><%= receipt.channel %></td>
        <td><%= receipt.user_id %></td>
        <td><%= link_to 'Show', receipt %></td>
        <td><%= link_to 'Edit', edit_receipt_path(receipt) %></td>
        <td><%= link_to 'Destroy', receipt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Receipt', new_receipt_path %>
