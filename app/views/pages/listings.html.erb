                           <div class="sc_property_icon">
                              <span class="icon-location"></span>
                           </div>
                           <div class="sc_property_title">
                              <div class="sc_property_title_address_1">
                                 <a href="single-post.html">
                                    <%= product.address %>
                                 </a> 
                              </div>
                              <div class="sc_property_title_address_2">
                                 <%= product.location %>
                              </div>
                           </div>
                           <div class="cL"></div>
                        </div>
                     </div>
                     <div class="sc_property_info_list">
                        <span class="icon-building113"><%= product.display_sqft %> sqft</span>
                        <span class="icon-bed"><%= product.bedroom %></span>
                        <span class="icon-bath"><%= product.bathroom %></span>
                        <!-- <span class="icon-warehouse">2</span> -->
                     </div>
                  </div>
               </div>
               <% end %>
